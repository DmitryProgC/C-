using System;
namespace Prog3
{
    class Program
    {
        static void Main(string[] args)
        {
            long num = 600851475143;
            for (int i = 2; i < num; i++)
            {
                if (num % i == 0)
                {
                    num = num / i;
                }  
            }
            Console.WriteLine(num);
        }
    }
}
